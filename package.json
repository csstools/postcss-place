  },
  "eslintConfig": {
    "extends": "10up",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "jscsConfig": {
    "preset": "10up"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "shorthands",
